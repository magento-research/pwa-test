<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="urn:magento:mftf:Page/etc/SectionObject.xsd">
    <section name="VeniaMainMenuSignInSection">
        <element name="mainArea" type="textarea" selector="[class*='navigation-signIn_open-']"/>
        <element name="email" type="input" selector="[class*='signIn-root'] input[name='email']"/>
        <element name="password" type="input" selector="[class*='signIn-root'] input[name='password']"/>
        <element name="signIn" type="button" selector="[class*='signIn-signInButton-'] button"/>
        <element name="forgotUsernamePassword" type="button" selector="//*[contains(text(), 'Forgot Password?')]"/>
        <element name="createAnAccount" type="button" selector="div[class^='signIn-createAccountButton'] button"/>
    </section>
</sections>
